.btnBox {
  position: fixed;
  bottom: 0;
  background-color: #ffffff;
  box-shadow: #B0C3FF 0px 0px 10px;
  width: 100%;
  height: 110rpx;
  display: flex;
  justify-content: space-between;
  text-align: center;
  line-height: 110rpx;
  letter-spacing: 2rpx;
}

.leftBtn {
  width: 65%;
  font-size: 30rpx;
  color: #333333;
}

.rightBtn {
  background-color: #F9795B;
  width: 35%;
  font-size: 32rpx;
  color: #ffffff;
  letter-spacing: 2px;
}

.partnerBtn {
  position: absolute;
  top: 610px;
  left: 0;
  right: 0;
  margin: 0 auto;
  border: 1px solid transparent;
  width: 490rpx;
  height: 92rpx;

}

.whiteBox {
  background-color: rgb(45, 127, 253);
  width: 100%;
  height: 100px;
}