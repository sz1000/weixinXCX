}
.demoBox:last-child{
    padding-right: 20rpx;
    border: none;
    
}
.imgView{
    margin: 20rpx 10rpx 0rpx 10rpx;
    display: flex;
    justify-content: space-around;
}
.demoImg{
    width:340rpx;
    height: 340rpx;
}
