.questionBox ,.answerBox{
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    line-height: 36rpx;
    letter-spacing: 4rpx;
    font-size: 26rpx;
    margin-bottom:20rpx;  
}
.questionBox image{
    width: 36rpx;
    height: 36rpx;
}
.answerBox p{
    color: #5297FC;
    width: 140rpx;
}
.answerBox{
    display: flex;
    flex-direction: row;
    align-items: flex-start;
}
.question{
    color: #000000;
    font-weight: bold;
}
.answer ,.question{
    margin-left: 15rpx;
}
.answer{
    width: 514rpx;
    height: fit-content;
    color: #666666;
}
.projectAreaBox{
    margin-bottom: 20px;
    border: none;
}
.projectBox {
    margin-top: 15px;
    border-bottom: 1px solid #EEEEEE;
    padding: 10rpx 30rpx;
    display: flex;
    justify-content: flex-start;
    padding-bottom: 40rpx;
  }
.projectCover {
    margin-right: 20rpx;
    min-width: 202rpx;
    max-width: 202rpx;
    width: 202rpx;
    height: 202rpx;
    border-radius: 6px;
  }

.projectInfo{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    padding: 10rpx 20rpx;
}
.projectTitle{
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    font-size: 26rpx;
    font-weight: 500;
    color: #222222;
  }
  .projectPrice{
    color: #f55f52;
    font-size: 14px;
    font-family: "PingFang-SC-Bold";
    overflow:hidden;
    text-overflow:ellipsis;
    display:-webkit-box;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:2;
}

.nodataContent{
    width: 100%;
    height: 60px;
    text-align: center;
    line-height: 60px;
    color: #333333;
    font-size: 14px;
}
.bindProject{
    width: 690rpx;
    height: 34rpx;
    margin-top: 22rpx;
    margin-left: 30rpx;
    padding-bottom: 30rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    color: #8a898a;
    border-bottom: 1rpx solid #eeeeee80;
    line-height: 30rpx;
  }
  .bindPrice{
    color:#ff668D;
    font-size: 32rpx;
  }
  .rightBlock{
    display: flex;
    flex-direction: row;
    align-items: center;
  }
  .buyIcon{
    width: 30rpx;
    height: 30rpx;
    background:linear-gradient(-90deg,rgba(255,105,157,1) 0%,rgba(255,145,148,1) 100%);
    border-radius: 2rpx;
    color: #ffffff;
    font-size: 20rpx;
    text-align: center;
    vertical-align: center;
    /* line-height: 28rpx; */
  }
  .projectName{
    width: 336rpx;
    font-size: 26rpx;
    margin-left: 10rpx;
  }
  .orderNum text{
    margin-left: 10rpx;
  }
  .showMask{
      width: 100%;
      height: 100%;
      position: fixed;
      top:0;
      left:0;
      background-color: #000000;
      opacity: 0.6;
  }
  .modalContainer{
      border-radius: 14px;
      width: 537rpx;
      height: fit-content;
      position: fixed;
      left: 106rpx;
      top:200rpx;
      overflow: hidden;
  }
  .credentialsBox{
      width: 537rpx;
      height: 784rpx;
      text-align: center;
        background-color: #ffffff;
  }
  .topImgBox{
      width: 101%;
      height: 191rpx;
      background-image: url(https://img.kodin.cn/3c830eb6d1ddcafda1623e4a010a900.png);
      background-size: 100% 100%;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      color: #ffffff;
      font-size: 30rpx;
  }
  .topImgBox :nth-child(1){
    border-top-left-radius: 12px;
    border-top-right-radius: 12px;
      color: #fff;
      font-size: 49rpx;
      font-weight: 500;
      text-shadow:0px 5px 21px rgba(0,123,188,0.75);
      margin-bottom: 22rpx;
  }
  .imgBox{
      width: 475rpx;
      height: 234rpx;
      margin: 38rpx auto 0 auto;
      background-color: #BFBFBF;

  }
  .cancel{
      width: 63rpx;
      height: 63rpx;
      margin-top: 43rpx;
      margin-left: 234rpx;
  }

  
