page{
  height: 100%;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.projectImg{
  width: 100vm;
  height: 670rpx;
}
.projectImg image{
  width: 100%;
  height: 100%;
}
.middleBar{
  margin: 30rpx 0 0 30rpx;
}
.productContent{
  width:690rpx;
  margin: 20rpx 0 0 30rpx;
  font-size: 28rpx;
  color: #333333;
  font-weight: 400;
  line-height: 44rpx;
  letter-spacing: 4rpx;
  padding-bottom: 50px;
}
.swiperBox{
  width: 100%;
  height: 88rpx;
  background-color: #fff;
  position: fixed;
  top: 0;
  left:0;
  z-index: 999;

}
swiper-item{
  overflow: scroll;
}

.swiper-tab{
  width: 70%;
  display: flex;
  text-align: center;
  justify-content: space-between;
  height: 88rpx;
  line-height: 88rpx;
  margin-bottom: 20px;
  position: fixed;
  left:0;
  top: 0;
  background-color: #fff;
  margin-left: 15%;
}

.swiper-tab-item{
  border-bottom: 1rpx solid #EEEEEE;
  display: inline-block;
  color:#888888;
  font-size: 14px;
  width: 100rpx;
}
.active{
  height:50rpx;
  padding-bottom:38rpx;
  color:#5297FC;
  border-bottom: 4rpx solid #5297FC;
}

swiper{
  height: 100%;
}

scroll-view{
  height:100%;
  margin-top: 80rpx;
}
.doctorBox{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.itemView{
  width: 44%;
  margin: 3%;
}
.itemView:last-child{
margin-bottom: 50px;
}
.doctorImg{
  /* min-width: 330rpx; */
  width: 330rpx;
  height: 330rpx;
  border-radius: 6rpx;
}
.doctor{
  margin-top: 20px;
  width: 345px;
  height:120px;
}
.doctorNames{
  font-size: 30rpx;
  color: #000000;
  text-align: left;
  /* margin: 4rpx 20rpx 4rpx 0rpx; */
  margin:20rpx 0 10rpx 0rpx;

}

.doctorNames span{
  font-size: 24rpx;
  color: #777777;
  margin-left: 20rpx;
}
.hospitalNames{
  color:#777777;
  text-align: left;
  font-size: 26rpx;
  letter-spacing: 2rpx;
}
.yynum span{
  color: #777777;
  font-size: 12px;
  line-height: 10px;
}
.order{
  margin-right: 15px;
}

.order:after{
  content: "|";
  margin-left: 15px;
  color: #EEEEEE;
}

.menu{
  width: 100%;
  height: 22px;
  line-height: 20px;
  margin-top: 20rpx;
  display: flex;
  flex-direction: row; 
  /* overflow: hidden; */
}
.item{
  /* margin-right: 8px; */
  background-color: #5297FC;
  width: fit-content;
  height: 18px;
  border-radius: 4px;
  font-size: 11px;
  color: #fff;
  line-height: 18px;
  padding:0 3px 0 3px;
  letter-spacing: 1px;
  font-family: "PingFang-SC-Regular";
  margin-right:20rpx;
  /* float: left; */
}

.demoBox{
  border-bottom: 8px solid #F3F4F5;
}
.demoBox:last-child{
  margin-bottom: 45px;
  border: none;
  /* padding-bottom: 45px; */
}
.personInfo{
  display: flex;
  align-items: center;
}
.demoBox:last-child .personInfo{
  border: none;
}
.demoImg{
  width: 170px;
  height: 170px;
  /* margin-left: 3px; */
  margin: 3%;
  margin-top:0;
}

.demoImg:last-child{
  margin-left: 0px;
  margin-right: 0;
}
.demoTitle{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  padding: 30rpx 3% 10rpx 3%;
  font-size: 30rpx;
  font-weight:600;
  line-height:50rpx ;
  color: #000000;
  letter-spacing: 3rpx;
}
.imgView{
  /* margin-left:14px; */
  /* margin: 0px 5px; */
  height: 170px;
  display: flex;
  justify-content: flex-start
}

.noDataBox{
  width:100%;
  height:250px;
  line-height:250px;
  text-align:center;
  font-size:13px;
  color:#454545;

}
.bindProject{
  width: 690rpx;
  height: 34rpx;
  margin-top: 22rpx;
  margin-left: 30rpx;
  padding-bottom: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  color: #8a898a;
  border-bottom: 1rpx solid #eeeeee80;
  line-height: 30rpx;
}
.bindPrice{
  color:#ff668D;
  font-size: 32rpx;
}
.rightBlock{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.buyIcon{
  width: 30rpx;
  height: 30rpx;
  background:linear-gradient(-90deg,rgba(255,105,157,1) 0%,rgba(255,145,148,1) 100%);
  border-radius: 2rpx;
  color: #ffffff;
  font-size: 20rpx;
  text-align: center;
  vertical-align: center;
  /* line-height: 28rpx; */
}
.projectName{
  width: 336rpx;
  font-size: 26rpx;
  margin-left: 10rpx;
}
.orderNum text{
  margin-left: 10rpx;
}
