/* 开始 */

.contentBox {
    display: flex;
}

.contentBox1 {
    transform: scale(0.7, 0.7);
    /* opacity: 0.8; */
}

.boxImg {
    position: relative;
    width: 100%;
    height: 80px;
    display: flex;
    background-color: #fff;
}

.uplodImg {
    margin: 10px 10px;
    width: 59px;
    height: 59px;
    background-size: 100%;
    background-repeat: no-repeat;
}

.countTxt {
    position: absolute;
    right: 22px;
    bottom: 6px;
    color: #a8a8a8;
    font-size: 23rpx
}

.innerTitle {
    width: 76%;
    position: static;
    padding-top: 8px;
    height: 70%;
    /* z-index: 1; */
    color: #bfbfbf;
    font-size: 26rpx;
    text-overflow: ellipsis;
    /* white-space:nowrap;  */
    overflow: hidden;
    padding-left: 15rpx !important;
}

.innerTitleText{
    width: 76%;
    position: static;
    padding-top: 8px;
    height: 70%;
    /* z-index: 1; */
    color: #bfbfbf;
    font-size: 26rpx;
    text-overflow: ellipsis;
    /* white-space:nowrap;  */
    overflow: hidden;
    margin-left:20rpx;
    display: flex;
    align-items: center;
}

/* 头部样式 */
.bigTitle {
    background-color: #fff;
    border: 1px solid #f5f5f5;
    height: 51px;
    line-height: 102rpx;
    font-size: 26rpx;
    color: #333333;
    padding-left: 15rpx;
}

.item {
    position: relative;
    height: 230rpx;
    line-height: 200rpx;
    /* overflow: hidden; */
}


.inner {
    position: relative;
    display: flex;
    /* top:0; */
    height: 200rpx;
}

.inner.txt {
    background-color: #fff;
    width: 100%;
    /* z-index: 5; */
    padding: 0 10rpx;
    transition: left 0.2s ease-in-out;
    white-space: nowrap;
    /* overflow:hidden; */
    text-overflow: ellipsis;
}


/* foot 按钮样式 */

.foot_btn {

    width: 100%;
    position: fixed;
    z-index: 9999;
    bottom: 0rpx;
    left: 0rpx;
}

.lookBtn,
.saveBtn {
    background-color: #ffffff;
    width: 50%;
    height: 100rpx;
    line-height: 50px;
    color: #333333;
    font-size: 24rpx;
    float: left;
    border-radius: 0rpx;
}

.saveBtn {
    background-color: #5297fc;
    color: #ffffff;
    float: right;

}

/* 添加按钮样式 */
.actionView {
    position: absolute;
    margin: 0 auto;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    /* margin-bottom: 200px; */
}

.addBtn,
.orderBtn {
    float: left;
    background-color: #ffffff;
    width: 134rpx;
    height: 46rpx;
    margin-top: 30rpx;
    border-radius: 67rpx;
    font-size: 20rpx;
    color: #999999;
    line-height: 46rpx;
    border: 0px solid;
    margin: 30rpx 50px 0 50px;
}


.orderBtnActive {
    background-color: #5297fc !important;
    color: #ffffff !important;
}

.placeholder {
    width: 100%;
    height: 200px;
}


/* 覆盖图层 */
.mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #222222;
    z-index: 9000;
    opacity: 0.2;
}

.modalDlg {
    width: 500rpx;
    height: 200rpx;
    position: fixed;
    top: 50%;
    left: 0;
    z-index: 9999;
    margin: -270rpx 125rpx;
    background-color: #fff;
    border-radius: 16rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.menuBtn {
    height: 100rpx;
    width: 499rpx;
    border-bottom: 1px solid #eeeeee;
    line-height: 100rpx;
    text-align: center;
    font-size: 30rpx;
}

.menuBtn:last-child {
    border-bottom: none;
}

.menuBtn image {
    width: 42rpx;
    height: 34rpx;
    margin-right: 20rpx;
    vertical-align: middle;

}

