.secname {
    font-size: 17px;
    line-height: 1.2;
}

.seccode {
    font-size: 13px;
    color: rgba(89, 89, 89, .6);
    line-height: 1.2;
}

.stay_bottom {
    height: 50px;
}

.bottom_row {
    width: 100%;
    overflow: hidden;
    background-color: #fff;
    line-height: 49px;
    height: 49px;
    border-top: 1px #a5a5a5 solid;
    position: fixed;
    z-index: 99;
    left: 0;
    bottom: 0;
    font-size: 17px;
    color: #929292;
}

.stay_bottom .all_select .ico_checker {
    background-position: 0 -17px;
