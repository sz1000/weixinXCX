page {
    background-color: #fff;
}

.touchView {
    z-index: 200;
    width: 100px;
    position: absolute;
    top: 0;
    left: 275px;
    top: 0;
}

/* 新滑动组建 */
.moveContentArea {
    width: 100%;
    height: 1000px;
    /* background-color: beige; */
}

.moveContentView {
    width: 100%;
    height: 80px;
}

.floatView {
    right: 0;
    width: 100%;
    height: 1000px;
    /* background-color: blue; */
    opacity: 1;
    position: absolute;
    z-index: 100;
}

.moveIng {
    width: 60px;
    height: 1000px;
    /* background-color: aquamarine; */
    position: absolute;
    right: 0;
    z-index: 200;
    opacity: 0.3;
}

.move {
    width: 100%;
    height: 90px;
    /* background-color: tomato; */
    position: absolute;
}

/* ------------------- */

.edit_row {
    width: 100%;
    overflow: hidden;
    font-size: 12px;
    height: 200rpx;
    box-sizing: border-box;
    padding: 15px 0;
    border-bottom: 1px rgb(185, 147, 147) solid;

}

.edit_col1 {
    float: left;
    width: 12%;
    text-align: center;
    padding-top: 40px;
}

.edit_col2 {
    float: left;
    width: 40%;

    padding-top: 40px;
}

.edit_col3 {
    float: left;
    width: 24%;
    text-align: center;
}

.edit_col4 {
    float: left;
    width: 23%;
    text-align: center;
    position: relative;
    padding-top: 40px;
}

.edit_del {
    float: left;
    padding-right: 10px;
    color: #fd7736;
}

.edit_finish {
    float: right;
    padding-right: 10px;
    width: 24%;
    height: 40px;
    color: #fd7736;
}

.edit_row_title .edit_col4:before {
    content: '';
    position: absolute;
    width: 1px;
    height: 18px;
    background-color: #e8e8e8;
    left: 0%;
    top: 50%;
    margin-top: -9px;
}

.edit_title_col1 {
    position: relative;
    left: 10px;
}

.stay_top {
    /* height: 151px; */
    margin-bottom: 10px;
    /* line-height: 47px; */
}

.stay_top .edit_row {
    height: 47px;
    padding: 0;
    position: fixed;
    z-index: 99;
    left: 0;
    top: 0;
    width: 100%;
    color: rgba(89, 89, 89, .8);
    font-size: 14px;
}

.edit_row_holder {
    background-color: #fff;
}

.edit_row_on .ico_checker {
    background-position: 0 -17px;
}

.moveable_row {
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 100;
}

.moveable_row .edit_row {
    box-shadow: 0 0 12px rgba(0, 0, 0, .2)
}

.dragSelected {
    background-color: #ccc;
}

.placeholder {
    display: inline-block;
    width: 1px;
    height: 1px;
}

