page {
  background-color: rgb(245, 245, 245);
}

.tabTop {
  position: fixed;
  width: 100%;
  top: 0;
  z-index: 999;
}

.scrollBox {
  position: absolute;
  top: 42px;
  height: calc(100% - 42px);
}

.img-Collections {
  height: 330rpx;
  position: relative;
  width: 99%;
  padding-left: 30rpx;
}

.img-Collections .mark {
  background-color: rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  color: white;
  font-size: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;

}

.img-Collections>view {
  position: relative;
  float: left;
  width: 220rpx;
  height: 155rpx;
  margin-left: 10rpx;
  /* border-radius: 10rpx; */
  margin-bottom: 22rpx;
}

.img-Collections>view>image {
  width: 100%;
  height: 100%;
}

.img-Collections>view:first-child {
  width: 440rpx;
  height: 330rpx;
  margin-top: 0;
  margin-left: 0;
}

.imgIcon {
  width: 340rpx;
  height: 280rpx;
  /* overflow: hidden; */
  /* background-size:100%;
  background-repeat: no-repeat; */
}


.spinShow {
  width: 168px;
  height: 45px;
  position: absolute;
  z-index: 1;
}

.swiper-tab {
  background-color: #ffffff;
  width: 100%;
  text-align: center;
  height: 88rpx;
  line-height: 88rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;

}

.swiper-tab-item {
  width: 25%;
  color: #222222;
  font-size: 26rpx;
  margin-left: 10rpx;
}

.swiper-tab-item:after {
  margin-left: 60rpx;
  content: "|";
  color: #eeeeee;
}

.swiper-tab-item:last-child:after {
  margin-left: 20rpx;
  content: "";
}

.active {
  color: #fc5e50;
}

swiper {
  text-align: center;

}

.showArea {
  margin-top: 130rpx;
  width: 100%;
  height: 550px;

  overflow: hidden;
}

.showAreaTw {
  border: 1px solid transparent;
  background-color: #ffffff;
  /* height: 650rpx; */
  width: 100%;
  margin-bottom: 30rpx;
  position: relative;
}

.myMaterialBtn {
  width: 87rpx;
  height: 87rpx;
  position: absolute;
  bottom: 58rpx;
  right: 30rpx;
}

.myMaterialBtn1 {
  width: 87rpx;
  height: 87rpx;
  position: fixed;
  bottom: 46%;
  right: 30rpx;
  z-index: 10000;
}

.title {

  width: 90%;
  height: 30px;
  margin: 30rpx 43rpx 20rpx 30rpx;
  font-size: 34rpx;
  font-weight: bold;
  color: #000000;
  text-align: left;
  letter-spacing: 1px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.card {
  width: 0;
  height: 0;
  border-width: 50px 0 50px 50px;
  border-style: solid;
  border-color: #fc5e50 transparent transparent;
  /*灰 透明 透明 */
  position: absolute;
  right: 0rpx;

}

.card text {
  width: 60rpx;
  position: absolute;
  right: 0rpx;
  bottom: 53rpx;
  font-size: 23rpx;
  color: #ffffff;

}

.content {
  width: 90%;
  height: 70rpx;
  margin: 30rpx 43rpx 20rpx 30rpx;
  font-size: 26rpx;
  color: #717171;
  text-align: left;
  letter-spacing: 1px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: pre-wrap
}

.img,
.imgRadio {
  width: 690rpx;
  height: 280rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  margin-left: 30rpx;
  margin-right: 230rpx;
}

.img view {
  background-repeat: no-repeat;
  width: 31%;
  height: 200rpx;
}

.personInfo {
  border-bottom: 1rpx solid #eeeeee80;
  /* margin-top: 21rpx; */
  height: 90rpx;
  line-height: 90rpx;
  text-align: left;
  color: #717171;
  font-size: 24rpx;
