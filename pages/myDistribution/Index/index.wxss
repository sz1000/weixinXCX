  justify-content: space-between;
}
.traning view{
  margin-right: 10px;
}
.teamItemBox{
  border-bottom: 1px solid #eeeeee;
  background-color: #fff;
  width: 100%;
  height: 64px;
}
.teamItemBox .contentBox{
  margin-top: 0px;
  justify-content: flex-start;
}
.teamItemBox  .headImg{
  margin: 11px 12px 10px 15px;
  width: 43px;
  height: 43px;
}
.teamItemBox .infoName{
  margin-bottom: 5px;
}
.teamItemBox .infoBox{
  margin-top: 14px;
  margin-left: 0px;
  height: 45px;
  font-size: 12px;
}
.getEarning{
  color: #F53A2F;
}
.noDataBox{
  width: 100%;
