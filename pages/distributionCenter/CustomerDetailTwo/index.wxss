/* pages/distributionCenter/CustomerDetailTwo/index.wxss */
.header{
width:94%;
height: 264rpx;
background: #C9A880;
border-radius: 20rpx;
margin: 60rpx auto;
display: flex;
flex-direction: column;
padding: 20rpx;
}
.Title_wrap{
  width: 94%;
  height: 150rpx;
  display: flex;
  justify-content: space-between;
  position: absolute;
  top:0%;
}

/* 头像 */
.portrait{
  width: 150rpx;
  height: 150rpx;
  background: #eee;
  border-radius:50%;
 
}
/* 一键拨号 */
.dial{
  width: 120rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  background:rgba(245,245,245,1);
 border-radius:21px;
 color:rgba(201,168,128,1);
 font-size: 24rpx;
 font-weight:400;
  border:1px solid rgba(255,255,255,1);
font-family:PingFang SC;

}
.userInfo_wrap{
  margin-top:100rpx;
  height: 200rpx;
  display: flex;
  flex-direction: row;
}
.grade_name_wrap{
  display: flex;
  flex-direction: column;
  padding: 10rpx;
}
.name{
  text-align: center;
  margin-bottom: 10rpx;
  color: #fff;
  font-family:PingFang SC;
  font-weight:500;
  font-size: 36rpx;
}

.userInfo_box{
  height: 50rpx;
  line-height: 50rpx;
  display: flex;
  flex-direction: column;
  margin-left: 108rpx;
  color: #fff;
}
/* 带来的收益 */
.performance_container{
  min-height: cac(100vh-264rpx);
  padding: 20rpx;

}
.title_wrap{
  height: 40rpx;
  line-height: 40rpx;
  display: flex;
  flex-direction: row;
}
.Title_icon{
  width: 6rpx;
  height: 30rpx;
  line-height: 30rpx;
  margin: 5rpx 10rpx;
  background:rgba(201,168,128,1);
}
.Total_performance{
  font-size: 32rpx;
  color:rgba(51,51,51,1);
  font-family:PingFang SC;
  font-weight:500;
  margin-left: 10rpx;
   
}
.performance_price_wrap{
   height: 80rpx;
   line-height:80rpx;
   display: flex;
   flex-direction: row;
}
.performance_price{
  font-size: 38rpx;
  font-weight: bold;
}
/* 更多信息 */
.more_warp{
  /* height: 60rpx;
  line-height: 60rpx; */
  display: flex;
  flex-direction: column;
  padding: 20rpx;
}

.more_message{
    font-size: 32rpx;
 color:rgba(51,51,51,1);
  font-family:PingFang SC;
   font-weight:500;
   margin-left: 10rpx;
}
.order_number{
   padding: 20rpx 0rpx 10rpx 20rpx;
}
.order_total{
   padding: 10rpx 20rpx;
}
