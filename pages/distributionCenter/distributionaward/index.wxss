
.heard{
width:96%;
height: 264rpx;
background: #C9A880;
border-radius: 20rpx;
margin: 20rpx auto;
display: flex;
flex-direction: row;
justify-content: space-around;
}
.state_wrap{
  width: 42%;
  height: 60rpx;
  display: flex;
  flex-direction: row;
  margin: 60rpx;
  font-size: 32rpx;
  color: #fff;
}
.rank_wrap{
    width: 48%;
    height: 160rpx;
    display: flex;
    flex-direction: column;
    margin: 40rpx;
    font-size: 28 rpx;
    color: #fff;
}
.direct{
  margin: 20rpx;
}
.level{
  margin: 0rpx 20rpx 20rpx 20rpx;
}
.team{
  display: flex;
  flex-direction: row;
  margin: 0rpx 20rpx 0rpx 20rpx;
}
/* 累计分销奖励 */
.main{
   min-height: calc(100vh - 264rpx);
  padding: 20rpx;
  background:#fff;
}
.container_warp{
  height: 120rpx;
  margin: 20rpx auto;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  
}
.container{
   height: 100rpx;
   display: flex;
   flex-direction: column;
   padding: 10rpx 10rpx;
}
.container_number{
  font-size: 26rpx;
  font-weight: bold;
   margin-bottom: 20rpx;
}
.container_time{
   margin-bottom: 20rpx;
}
/* 佣金结算 */
.context_warp{
  height: 140rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.context_left{
  width: 45%;
  background: #F5F5F5;
  border-radius: 6rpx;
  padding: 30rpx;
  display: flex;
  flex-direction: column;
}
.context_right{
  width: 45%;
  background: #F5F5F5;
  border-radius: 6rpx;
   padding: 30rpx;
  display: flex;
  flex-direction: column;
}
.context_title{
  margin-left: 20rpx;
  margin-bottom: 20rpx;
  font-size: 26rpx;
  font-weight:400;
}
.context_num{
   margin-left: 20rpx;
  margin-bottom: 20rpx;
  font-size: 32rpx;
  font-weight:500;
color:rgba(199,166,126,1);
}