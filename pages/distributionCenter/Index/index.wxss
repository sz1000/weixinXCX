.title {
  font-size: 12px;
}

page {
  height: 100%;
  width: 100vm;
  background-color: #f5f5f5;
}

.userInfo {
  position: relative;
  width: 100vm;
  /* height: 340rpx; */
  background-color: #282828;
  display: flex;
  flex-direction: row;
  align-items: center;
  /* justify-content: center; */
  color: #fff;
  font-size: 34rpx;
  font-weight: 300;
  background-size: cover;
  background-repeat: no-repeat;
}

.user-headImg {
  /* margin-top: 37rpx; */
  margin:20rpx 20rpx 30rpx 60rpx;
}

.user-title {
  width: 206rpx;
  height: 47rpx;
  background-size: contain;
  background-repeat: no-repeat;
  font-size: 24rpx;
  line-height: 47rpx;
  text-align: center;
  margin:6rpx 30px 10rpx 0rpx;
 
}

.user-title view {
  margin-top: -1px;
}
.userbox{
  display: flex;
  flex-direction:column;
}
.statusbox{
  display: flex;
  flex-direction: row;
}
.user-name {
  font-size: 32rpx;
  font-weight: 300;
  margin-bottom: 20rpx;
  text-shadow: 1px 4px 10px rgba(120, 140, 171, 0.4);
}

.backDotsBox {
  width: 128rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  position: absolute;
  left: 11rpx;
  top: 30rpx;
}

.login-button {
  width: 150rpx;
  height: 30px;
  line-height: 30px;
  font-size: 12px;
  background-color: #fff;
}

.userInfo image {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  margin-bottom: 30rpx;
}

.userInfo .setUp {
  width: 38rpx;
  height: 38rpx;
  position: absolute;
  top: 29rpx;
  right: 29rpx;
}

.container {
  position: absolute;
  left: 28rpx;
  top: 240rpx;
}

.commonBox {
  width: 694rpx;
  height: 300rpx;
  background-color: rgb(255, 255, 255);
  border-radius: 14rpx;
  margin-bottom: 10px;
}
.invite_btn{
  margin-top:46rpx;
}

.commonBox1 {
  overflow: hidden;
}

.item1 {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.common-bar-left {
  display: flex;
  align-items: center;
}

.commonIcon {
  width: 8rpx;
  height: 30rpx;
  background: linear-gradient(0deg, rgba(194, 161, 119, 1) 0%, rgba(230, 196, 166, 1) 99%);
  border-radius: 14rpx;
  margin-right: 20rpx;
  margin-left: -4rpx;
}

.saler-title {
  margin: 32rpx auto;
  text-align: center;
  font-size: 36rpx;
  font-weight: 500;
  color: rgba(200, 167, 128, 1);
}

.saler-title::after {
  position: absolute;
  top: 64rpx;
  left: 274rpx;
  content: '';
  width: 141rpx;
  height: 17rpx;
  background: linear-gradient(45deg, rgba(194, 161, 119, 0.6) 0%, rgba(230, 196, 166, 0) 99%);
  border-radius: 9rpx;
}

.saler-container {
  width: 693rpx;
  margin: 40rpx auto;
  background: rgba(255, 255, 255, 1);
  border-radius: 14px;
  overflow: hidden;
  position: relative;
}

.commonBar {
  height: 89rpx;
  color: #222;
  font-size: 30rpx;
  font-weight: 500;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin: 0 20rpx;
}

.showOrderBtn {
  color: #999;
  font-size: 24rpx;
  font-weight: 400;
}

.showOrderBtn image {
  width: 9rpx;
  height: 14rpx;
}

.itemBox {
  width: 574rpx;
  height: fit-content;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
  align-items: center;
  margin: 60rpx 0 0 60rpx;
}

.saler-item-box {
  margin-top: 30rpx;
}

.item {
  width: 170rpx;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  color: #222;
  font-size: 24rpx;
  margin-bottom: 30rpx;
}


.item image {
  width: 48rpx;
  height: 48rpx;
  margin-bottom: 30rpx;
}

.item-bg {
  width: 72rpx;
  height: 72rpx;
}

.itemNum {
  display: block;
  width: 26rpx;
  height: 26rpx;
  text-align: center;
  line-height: 24rpx;
  background-color: #fc5e50;
  color: #fff;
  box-shadow: 0px 2px 4px 0px rgba(247, 104, 87, 0.4);
  border-radius: 50%;
  position: absolute;
  top: 0;
  right: 24rpx;
}

.salerBox {
  width: 694rpx;
  height: 280rpx;
  background-color: #fff;
  margin: 292rpx 0 0 28rpx;
  border-radius: 14rpx;
}

.item span {
  color: #c9a880;
  font-size: 37rpx;
  font-weight: bold;
  /* font-weight: normal; */
  margin-bottom: 22rpx;
}

.salerToolsBox {
  width: 694rpx;
  height: fit-content;
  background-color: #fff;
  margin: 22rpx 0 0 28rpx;
  border-radius: 14rpx;
  margin-bottom: 40rpx;
}

.redpacket_fill {
  font-size:32rpx;
  font-family:Alibaba   PuHuiTi;
  font-weight:400;
  color:rgba(48,49,51,1);
  margin-right: 40rpx;
  
}
.redpacket_number{
  font-size:32rpx;
  font-weight:400;
  color:#C7A67E;
}