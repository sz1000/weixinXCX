page {
  font-size: 30rpx;
}

.hearder {
  height: 240rpx;
  background: linear-gradient(133deg, rgba(230, 196, 166, 1) 0%, rgba(194, 161, 119, 1) 100%);
  display: flex;
  flex-direction: column;
}

.earnings_wrap {
  height: 200rpx;
  padding: 20rpx;
}

.earnings_title {
  font-size: 60rpx;
  color: #fff;
  font-weight: bold;
  margin: 20rpx 0rpx;
  text-align: center;
}

.earnings_text {
  font-size: 26rpx;
  color: #fff;
  margin: 30rpx 0rpx 20rpx 0rpx;
  text-align: center;
}

.content {
  position: absolute;
  top: 200rpx;
  left: 0;
  background: #fff;
  border-radius: 2%;
  margin: auto 30rpx;
  width: 690rpx;
}

.main {
  min-height: cac(100vh-240rpx);
  padding: 20rpx;
}

.container_wrap {
  height: 200rpx;
  position: relative;
  padding: 0 40rpx;
}

.container_box {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.money_incon {
  font-size: 46rpx;
  font-weight: bold;
  margin: 20rpx 10rpx;
}

/* 请输入金额 */

.input_money {
  font-size: 46rpx;
  margin: 26rpx 20rpx;
}

.input_money>input {
  height: 60rpx;
  line-height: 60rpx;
  display: block;
  font-size: 30rpx;
}

.btn_money {
  width: 108rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin: 28rpx 10rpx;
  background: rgba(201, 167, 128, 1);
  border-radius: 24px;
  font-size: 24rpx;
  color: #fff;
  padding: 8rpx 20rpx;
}

.price {
  color: red;
}

.container_text {
  margin: 20rpx 0rpx 0px 10rpx;
  font-size: 22rpx;
  color: #666;
}

/* 收款信息 */

.context_wrap {
  display: flex;
  flex-direction: column;
}

.context_title {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: rgba(48, 49, 51, 1);
  padding: 20rpx;
}

.context_region {
  display: flex;
  flex-direction: row;
  align-content: center;
  font-weight: 400;
  color: rgba(144, 147, 153, 1);
  height: 40rpx;
  line-height: 40rpx;
  padding: 20rpx;
}

/* 收款账户 */

.context_title {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: rgba(48, 49, 51, 1);
}

.context_region {
  display: flex;
  flex-direction: row;
  align-content: center;
  font-weight: 400;
  color: rgba(144, 147, 153, 1);
  height: 40rpx;
  line-height: 40rpx;
  padding: 20rpx;
}

.cotext_icon {
  width: 40rpx;
  height: 40rpx;
  background: #eee;
  border-radius: 50%;
}

.context_tel {
  margin-left: 10rpx;
}

.image {
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
}

/* 提交 */

.footer {
  margin: 160rpx 10rpx 30rpx;
}

.submit_btn {
  height: 80rpx;
  line-height: 80rpx;
  background: rgba(194, 161, 119, 1);
  border-radius: 6px;
  color: #fff;
  text-align: center;
  margin: auto;
}

/* 弹框 */

.img_wrap {
  display: flex;
  flex-direction: column;
  position: absolute;
  top: 20%;
  left: 20%;
  z-index: 10000;
}

.account_money {
  font-size: 46rpx;
  color: red;
  height: 40rpx;
  line-height: 40rpx;
  position: absolute;
  top: 44%;
  left: 33%;
}

.procedurefree_wrap {
  display: flex;
  flex-direction: row;
  padding: 20rpx 90rpx;
}

.procedurefree {
  font-size: 20rpx;
  color: rgba(181, 132, 86, 1);
  margin-right: 20rpx;
  position: absolute;
  top: 56%;
  left: 24%;
}

.rate {
  font-size: 20rpx;
  color: rgba(181, 132, 86, 1);
  margin-left: 20rpx;
  position: absolute;
  top: 56%;
  left: 50%;
}

/* 取消 */

.cancel {
  font-size: 24rpx;
  width: 110rpx;
  height: 48rpx;
  line-height: 48rpx;
  color: #fff;
  background: linear-gradient(90deg, rgba(65, 66, 68, 1) 0%, rgba(96, 97, 100, 1) 100%);
  border-radius: 30rpx;
  text-align: center;
  position: absolute;
  top: 76%;
  left: 60%;
}

/* 确定 */

.confirm {
  width: 110rpx;
  height: 48rpx;
  font-size: 24rpx;
  line-height: 48rpx;
  color: #fff;
  background: linear-gradient(90deg, rgba(255, 150, 41, 1) 0%, rgba(254, 66, 90, 1) 100%);
  box-shadow: 1px 6px 19px 1px rgba(254, 75, 84, 0.3);
  border-radius: 30rpx;
  text-align: center;
  position: absolute;
  top: 76%;
  left: 18%;
}
.mask {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  background: #666;
  opacity: .5;
  z-index: 999;
}