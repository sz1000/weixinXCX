}

.img-Collections {
  height: 330rpx;
  position: relative !important;
  width: 99%;
}

.img-Collections .mark {
  background-color: rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  color: white;
  font-size: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;

}

.img-Collections>view {
  position: relative;
  float: left;
  width: 220rpx;
  height: 155rpx;
  margin-left: 10rpx;
  /* border-radius: 10rpx; */
  margin-bottom: 22rpx;
}

.img-Collections>view>image {
  width: 100%;
  height: 100%;
}

.img-Collections>view:first-child {
  width: 440rpx;
  height: 330rpx;
  margin-top: 0;
  margin-left: 0;
}


