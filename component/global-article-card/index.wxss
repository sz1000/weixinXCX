
.container{
  width: 100%;
  overflow-x: hidden;
}

.ellipsisOne {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}


.showAreaTw {
  border: 1px solid transparent;
  background-color: #ffffff;
  width: calc(100% - 60rpx);
  /* margin-bottom: 30rpx; */
  position: relative;
  padding: 0 30rpx;
}

.title {

  width: 90%;
  height: 30px;
  margin-top: 31rpx;
  font-size: 34rpx;
  line-height: 42rpx;
  font-weight: 400;
  color: #000000;
  text-align: left;
  letter-spacing: 2px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.sub-title {
  width: 100%;
  margin-bottom: 17rpx;
  font-size: 24rpx;
  color: #717171;
  text-align: left;
  letter-spacing: 1px;
  line-height:32rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.img-container {
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.img-item {
  width: 49%;
  height: 280rpx;
}



.personInfo image {
  border-radius: 100%;
  width: 48rpx;
  height: 48rpx;
  vertical-align: middle;
  margin-right: 14rpx;
  object-fit: cover;
}

.bindProject-item {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.buyIcon {
  width: 30rpx;
  height: 30rpx;
  background: linear-gradient(-90deg, rgba(255, 105, 157, 1) 0%, rgba(255, 145, 148, 1) 100%);
  border-radius: 2rpx;
  color: #ffffff;
  font-size: 20rpx;
  text-align: center;
  vertical-align: center;
  /* line-height: 28rpx; */
}

.projectName {
  width: 336rpx;
  font-size: 26rpx;
  line-height: 36rpx;
  margin-left: 10rpx;
}

.orderNum text {
  margin-left: 10rpx;
}



.bindPrice {
  color: #ff668D;
  font-size: 32rpx;
}

.bindPrice span {
  font-size: 24rpx;
}

.personInfoBox {
  clear: both;
  width: 690rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: left;
  color: #717171;
  font-size: 24rpx;
  border-bottom: 1rpx solid #eeeeee80;
  display: flex;
  align-items: center;
}

.personInfoBox image {
  border-radius: 100%;
  width: 48rpx;
  height: 48rpx;
  margin-right: 14rpx;
  vertical-align: middle;
}
.menuJoin {
  margin-left: 30rpx;
  height: 90rpx;
  background-color: #ffffff;
  width: 690rpx;
  text-align: center;
  line-height: 100rpx;
