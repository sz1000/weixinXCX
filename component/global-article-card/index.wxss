  flex-flow: row;
  justify-content: center;
  align-items: center;
}

.imgAdd {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  margin-right: 12rpx;
}



.joinFontColor {
  color: #FC5E50;
}
.caseItem {
  margin-top: 30rpx;
}

.caseItemImgBox {
  width: 100%;
  height: 100%;
  margin-top: 25rpx;
  display: flex;
  justify-content: space-between;
}

.caseItemImg {
  width: 348rpx;
  height: 348rpx;
}

.caseItemUser {
  width: 100%;
  height: 102rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #EEEEEE80;
}

.caseItemUserImg {
  width: 48rpx;
  height: 48rpx;
  border-radius: 24rpx;
}

.caseItemUserName {
  color: #666666;
  font-size: 12px;
  margin-left: 15rpx;
}

.showAreaRadio {
  height: 635rpx;
}

.CardArea {
  /* width: 100%; */
