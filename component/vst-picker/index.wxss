
.pickerStyle {
  width: 100%;
  height: 44px;
  margin: 0;
  padding: 4px 0 0 0;
  border-radius: 0rpx;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-left: 16px;
  border-bottom: 1rpx solid #eeeeee;
}

.pickerTitle {
  width:690rpx;
  color: #222222;
  font-size:32rpx;
  padding-bottom: 4px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.pickerTitle input {
  color: #495060;
  margin: 0;
  height: 40px;
}
.right{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.pickerTitle image{
  width: 16rpx;
  height: 26rpx;
  margin-left: 14rpx;
}
.pickerTitle view {
  min-width:65px;
  /* padding-right:10px; */

}

input{
  text-align: right;
  width: 373rpx;
}